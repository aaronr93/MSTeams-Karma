{
  "$schema": "https://statics.teams.microsoft.com/sdk/v1.3.0-beta.2/manifest/MicrosoftTeams.schema.json",
  "manifestVersion": "1.3",
  "version": "0.0.0",
  "id": "(example)",
  "packageName": "com.package.name",
  "developer": {
    "name": "Aaron",
    "websiteUrl": "https://github.com/aaronr93/MSTeams-Karma",
    "privacyUrl": "https://github.com/aaronr93/MSTeams-Karma",
    "termsOfUseUrl": "https://github.com/aaronr93/MSTeams-Karma"
  },
  "name": {
    "short": "MSTeams-Karma",
    "full": "MSTeams-Karma"
  },
  "description": {
    "short": "This is a small sample app we made for you!",
    "full": "This is a small sample app we made for you! This app has samples of all capabilities MS Teams supports."
  },
  "icons": {
    "outline": "bot_blue.png",
    "color": "bot_blue.png"
  },
  "accentColor": "#3F487F",
  "bots": [
    {
      "botId": "(example)",
      "isNotificationOnly": false,
      "scopes": [ "team", "groupchat" ],
      "commandLists": [
        {
          "scopes": [
            "team",
            "groupchat",
            "personal"
          ],
          "commands": [
            {
              "title": "get top",
              "description": "Get the top Karma"
            },
            {
              "title": "get bottom",
              "description": "Get the bottom Karma"
            }
          ]
        }
      ]
    }
  ],
  "permissions": [
  ],
  "validDomains": [
  ]
}